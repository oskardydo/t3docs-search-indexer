<div class="aggregations" id="suggestions" role="tablist">
    <div class="card">
        <div class="card-header p-0">
            <h2 class="mb-0 d-grid">
                <button
                        type="button"
                        class="btn text-primary aggregations-toggle-body"
                        data-text-hide="Hide suggestions"
                        data-text-show="Show suggestions"
                        onclick="document.getElementById('suggestions').classList.toggle('is-expanded')"
                ><i class="fa fa-chevron-down"></i></button>
            </h2>
        </div>
        <div class="card-body">
            <div class="suggestions-header">
                <span class="fs-3 fw-bold aggregation-title-text"> Suggestions</span>
            </div>
            {% for suggestKey,suggestions in searchSuggest %}
                {% if suggestions is not empty %}
                    <div class="suggestion-container" style="list-style-type:none">
                        <span class="fs-4 fw-bold aggregation-title-text">{{ getLabelForFilter(suggestKey) }}</span>
                        <div class="suggestion-rows">
                            {% for key,suggestValue in suggestions %}
                                <div class="suggestion-row">
                                    <a class="link" href="{{ generateLinkWith(demand,suggestKey,suggestValue) }}">{{ suggestValue }}<span class="arrow"></span></a>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                {% endif %}
            {% endfor %}
        </div>
    </div>
</div>
